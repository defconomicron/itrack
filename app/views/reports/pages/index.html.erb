<table>
  <tr>
    <th>Page</th>
    <th>Hits</th>
    <th>Visits</th>
    <th>Visitors</th>
    
  </tr>
  <% @hits.each do |hit| %>
    <tr>
      <td><%=link_to hit.page || "", params.merge(:action => :set_page, :page => hit.page) %></td>
      <td><%= hit.hits %></td>
      <td><%= hit.visits %></td>
      <td><%= hit.visitors %></td>
    </tr>
  <% end %>
</table>
<%= will_paginate @hits %>
